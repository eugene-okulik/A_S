import itertools

def fib():

a = 0

b = 1

while True:

a, b = b, a + b

index1, index2, index3, index4 = 5, 200, 1000, 100000

lst = [next(itertools.islice(fib(), index1, None)), 
next(itertools.islice(fib(), index2, None)),
next(itertools.islice(fib(), index3, None)),
next(itertools.islice(fib(), index4, None))]

print(lst)
